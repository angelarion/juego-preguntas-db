apiVersion: v1
kind: Service
metadata:
  name: mysql-service-app-juego
  labels:
    app: mysql
spec:
  selector:
    app: mysql-app
  ports:
  - protocol: TCP
    port: 3306
    targetPort: 3306
    nodePort: 30006  # Puedes especificar un puerto NodePort específico, o dejar que Kubernetes asigne uno automáticamente
  type: NodePort
